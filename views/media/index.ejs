<h1>All Media</h1>

<ul>
  <% media.forEach((mediaItem) => { %>
    <li>
      <a href="/media/<%= mediaItem._id %>"><%= mediaItem.title %> (<%= mediaItem.year ? mediaItem.year : "NaN" %>)</a>
      (<%= mediaItem.type %>)
    </li>
  <% }); %>
</ul>


<a href="/media/new">Add a new media</a>
<a href="/">Back to home</a>